if shotsNeeded &amp;&amp; weaponCooldown {
    // Bullet
    scanDir = rotation + random_range(-5,5);
    scanX = x;
    scanY = y;
    changeX = lengthdir_x(8,scanDir);
    changeY = lengthdir_y(8,scanDir);
    
