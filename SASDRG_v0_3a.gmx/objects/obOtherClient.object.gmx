                distance = max(distance,0.25);
                scanX += changeX * distance;
                scanY += changeY * distance;
            }
        }
        draw_vertex(scanX,scanY);
    }
    draw_primitive_end();
    surface_reset_target();
    draw_set_blend_mode(bm_normal);
