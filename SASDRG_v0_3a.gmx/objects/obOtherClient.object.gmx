        scanX += changeX;
        scanY += changeY;
        velocity -= 1;
        for (i = 0; i &lt; 1; i += 0.2) {
            part_particles_create(obPart.part,scanX + i * changeX,scanY+ i * changeY,obPart.bullet,1);
