            <string>/// Movement
moveSpeed = moveSpeedBase;
if (keyboard_check(vk_shift) &amp;&amp; speed &gt; 0.1 &amp;&amp; energy){
    moveSpeed = moveSpeedBase * sprintMultiplier;
