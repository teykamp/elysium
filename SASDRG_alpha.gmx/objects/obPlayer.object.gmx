        y += sign(vspeed);
    }
    vspeed = 0;
}
if place_meeting(x + hspeed,y + vspeed,obWall) {
    vspeed = 0;
    hspeed = 0;
} else if place_meeting(x + hspeed,y + vspeed,obWall) {
    vspeed = 0;
    hspeed = 0;
}
